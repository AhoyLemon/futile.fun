aside.store(v-cloak v-if="availableUpgrades.length > 0")
  .header Buy Upgrades
  .animated.zoomInDown(v-for="item,i in availableUpgrades" @click="buyItem(i,item)")

    a.item(v-bind:class="{ buyable: score >= item.price}")
      .name-and-desc
        .name {{item.name}}
        .desc {{item.desc}}
      .price {{item.price}}